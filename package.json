  "scripts": {
    "build": "nest build --builder swc --type-check",
    "start": "dotenvx run -- nest start --builder swc",
    "start:dev": "dotenvx run -- npm run start -- --watch",
    "start:debug": "dotenvx run -- npm run start:dev -- --debug",
    "start:prod": "dotenvx run -- node dist/main",
    "dev": "dotenvx run -- nest start --builder swc --watch",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "test": "tsc --noEmit && dotenvx run -f .env.test -- jest",
    "test:watch": "dotenvx run -f .env.test -- jest --watch",
    "test:cov": "dotenvx run -f .env.test -- npm run test -- --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "11.0.12",
    "@nestjs/core": "11.0.12",
    "@nestjs/config": "4.0.2",
    "@nestjs/typeorm": "11.0.0",
    "@nestjs/platform-express": "11.0.12",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "class-validator": "0.14.1",
    "@nestjs/swagger": "11.0.7",
    "@nestjs/axios": "4.0.0",
    "pg": "8.14.1",
    "class-transformer": "0.5.1"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.51.2",
    "@swc-node/register": "1.10.10",
    "@swc/cli": "0.6.0",
    "@swc/core": "1.11.21",
    "@swc/jest": "0.2.37",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@nestjs/cli": "11.0.6",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "11.0.16",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "eslint": "9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-typescript": "4.3.2",
    "eslint-plugin-import-x": "^4.10.3",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-promise": "^7.2.1",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "typescript-eslint": "^8.50.1",
    "globals": "16.0.0"
  }
}
